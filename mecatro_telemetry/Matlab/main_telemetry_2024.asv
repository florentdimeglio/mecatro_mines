% Constants
IP = '192.168.4.1';
PORT_NUMBER = 80;
Tmax = 4;

[log_time, data_values, line_idx] = get_data_2024(IP, PORT_NUMBER, Tmax);

%Display logged variable names
disp(data_values.keys)  

% Plot logged variables
figure; 
hold on;
legend_labels = data_values.keys;
for i = 1:numel(legend_labels)
    d = legend_labels{i};
    v = data_values(d);
    plot(log_time, v, 'DisplayName', d);
end
hold off;
legend('Location', 'best');
grid on;


